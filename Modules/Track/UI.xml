<GuiXml>
  <Controls>
    <Backdrop name="BearSynergiesTrackBackdrop" centerColor="000000" mouseEnabled="true" movable="true" clampedToScreen="true" resizeToFitDescendents="true">
      <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="TOPLEFT" />
      <OnMoveStop>
        BearSynergies.Track.OnMoveStop()
      </OnMoveStop>
    </Backdrop>

    <Control name="BearSynergiesTrackCooldown" virtual="true" hidden="true">
      <Controls>
        <Texture name="$(parent)Icon" textureFile="esoui/art/icons/ability_undaunted_004.dds">
          <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="5" offsetY="5" />
          <Dimensions x="48" y="48" />
        </Texture>
        <Label name="$(parent)Timer" font="$(GAMEPAD_MEDIUM_FONT)|$(KB_26)|thick-outline" color="FFFFFF" text="0">
          <Anchor point="CENTER" relativeTo="$(parent)Icon" relativePoint="CENTER" />
        </Label>
      </Controls>
    </Control>





